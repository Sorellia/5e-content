{
  "id": "5e-content",
  "title": "5e Content",
  "description": "A consolidation of various content for my 5e campaigns",
  "library": false,
  "socket": true,
  "version": "#{VERSION}",
  "compatibility": {
    "minimum": "11",
    "verified": "11"
  },
  "authors": [
    {
      "name": "Airwolf",
      "url": "https://github.com/Sorellia"
    }
  ],
  "relationships": {
    "systems": [{
        "id": "dnd5e",
        "type": "system",
        "compatibility": {
          "minimum": "2.2.1",
          "verified": "2.2.2"
        }
      }
    ],
    "requires": [{
      "id": "midi-qol",
      "type": "module",
      "manifest": "https://gitlab.com/tposney/midi-qol/raw/v10/package/module.json",
      "compatibility": {
        "verified": "10.0.57",
        "minimum": "10.0.46"
      }
    },
    {
      "id": "dfreds-convenient-effects",
